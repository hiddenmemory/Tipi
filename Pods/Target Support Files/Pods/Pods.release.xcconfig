GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/OCDiscount" "${PODS_ROOT}/Headers/Public/RegexKitLite"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/OCDiscount"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/OCDiscount" -isystem "${PODS_ROOT}/Headers/Public/RegexKitLite"
OTHER_LDFLAGS = $(inherited) -ObjC -l"OCDiscount" -l"RegexKitLite" -l"discount" -l"icucore" -framework "Foundation"
PODS_ROOT = ${SRCROOT}/Pods